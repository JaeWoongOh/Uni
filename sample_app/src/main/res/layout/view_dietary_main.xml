            <!-- 오늘의 영양소 섭취 비율 -->

            <RelativeLayout
				android:id="@+id/rl_3nutrient"
                android:layout_width="match_parent"
                android:layout_height="130dp"
                android:layout_marginLeft="1dp"
                android:layout_marginRight="1dp"
                android:layout_marginTop="3dp"
                android:background="@drawable/list_box_bg_green" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="38dp"
                        android:orientation="horizontal" >

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="13dp"
                            android:layout_marginTop="8.5dp"
                            android:src="@drawable/ic_list_nutrient_green" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="8.5dp"
                            android:text="@string/dietary_nutrientrate"
                            android:textColor="#508026"
                            android:textSize="16dp" />
                    </LinearLayout>

                    <!-- 영양소 섭취 비율 -->

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp" >

                        <TextView
                            android:id="@+id/ratio_nutrients_general_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="41dp"
                            android:text="@string/dietary_rate_reasonable"
                            android:textColor="#4c4c4c"
                            android:textSize="14dp" />

                        <LinearLayout
                            android:id="@+id/ratio_nutrients_general_graph"
                            android:layout_width="240dp"
                            android:layout_height="9.5dp"
                            android:layout_marginLeft="8dp"
                            android:layout_marginRight="20dp"
                            android:layout_toRightOf="@id/ratio_nutrients_general_text"
                            tools:ignore="Orientation">
                            </LinearLayout>
                        <!-- android:background="#aaaaaa" -->

                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp" >

                        <TextView
                            android:id="@+id/ratio_nutrients_intake_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="41dp"
                            android:text="@string/dietary_rate_intake"
                            android:textColor="#596167"
                            android:textSize="14dp" />

                        <LinearLayout
                            android:id="@+id/ratio_nutrients_intake_graph"
                            android:layout_width="240dp"
                            android:layout_height="9.5dp"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="8dp"
                            android:layout_marginRight="20dp"
                            android:layout_toRightOf="@id/ratio_nutrients_intake_text" android:orientation="horizontal">
                        </LinearLayout>
                    </RelativeLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right|center_vertical"
                        android:layout_marginBottom="16dp"
                        android:layout_marginRight="20dp"
                        android:layout_marginTop="6dp"
                        android:orientation="horizontal" >

                        <View
                            android:layout_width="11dp"
                            android:layout_height="11dp"
                            android:background="#58991f" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="-4dp"
                            android:text="@string/dietary_carbohydrate"
                            android:textColor="#4d4d4d"
                            android:textSize="12dp" />

                        <View
                            android:layout_width="11dp"
                            android:layout_height="11dp"
                            android:layout_marginLeft="6dp"
                            android:background="#8bcc52" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="-4dp"
                            android:text="@string/dietary_protein"
                            android:textColor="#4d4d4d"
                            android:textSize="12dp" />

                        <View
                            android:layout_width="11dp"
                            android:layout_height="11dp"
                            android:layout_marginLeft="6dp"
                            android:background="#c1e5a1" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="-4dp"
                            android:text="@string/dietary_fat"
                            android:textColor="#4d4d4d"
                            android:textSize="12dp" />
                    </LinearLayout>
                </LinearLayout>
            </RelativeLayout>


            <!-- 나의 식이 개선 목표 -->

            <RelativeLayout
                android:id="@+id/rl_DietaryGoal"
                android:layout_width="match_parent"
                android:layout_height="115dp"
                android:layout_marginLeft="2.5dp"
                android:layout_marginRight="2.5dp"
                android:layout_marginTop="3dp"
                android:layout_marginBottom="3.5dp"
                android:background="#999999" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="31dp"
                        android:orientation="horizontal" >

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="13dp"
                            android:layout_marginTop="7dp"
                            android:src="@drawable/ic_list_flag_green" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="7dp"
                            android:text="@string/dietary_improvegoal"
                            android:textColor="#ffffff"
                            android:textSize="16dp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="85dp"
                        android:background="@drawable/text_box_bg"
                        android:gravity="center_vertical"
                        android:orientation="vertical" >

                        <!-- 1 -->

                        <LinearLayout
                            android:id="@+id/llDietaryGoal1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal" >

                            <ImageView
                                android:layout_width="7dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:layout_marginLeft="24dp"
                                android:src="@drawable/ic_list_text_point_round_green" />

                            <TextView
                                android:id="@+id/tv_DietaryGoal1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="10dp"
                                android:layout_marginRight="24dp"
                                android:singleLine="true"
                                android:textColor="#4d4d4d"
                                android:textSize="13dp" />
                        </LinearLayout>

                        <!-- 2 -->

                        <LinearLayout
                            android:id="@+id/llDietaryGoal2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:orientation="horizontal" >

                            <ImageView
                                android:layout_width="7dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:layout_marginLeft="24dp"
                                android:src="@drawable/ic_list_text_point_round_green" />

                            <TextView
                                android:id="@+id/tv_DietaryGoal2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="10dp"
                                android:layout_marginRight="24dp"
                                android:singleLine="true"
                                android:textColor="#4d4d4d"
                                android:textSize="13dp" />
                        </LinearLayout>

                        <!-- 3 -->

                        <LinearLayout
                            android:id="@+id/llDietaryGoal3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:orientation="horizontal" >

                            <ImageView
                                android:layout_width="7dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:layout_marginLeft="24dp"
                                android:src="@drawable/ic_list_text_point_round_green" />

                            <TextView
                                android:id="@+id/tv_DietaryGoal3"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="10dp"
                                android:layout_marginRight="24dp"
                                android:singleLine="true"
                                android:textColor="#4d4d4d"
                                android:textSize="13dp" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <TextView
                    android:id="@+id/tv_goal_emtry"
                    android:layout_width="wrap_content"
                    android:layout_height="80dp"
					android:layout_marginTop="30dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginLeft="27dp"
					android:layout_marginRight="27dp"
                    android:gravity="center_vertical"
                    android:text="@string/dietary_goal_emtry"
                    android:textColor="#999999"
                    android:textSize="14dp" />

                <ImageButton
                    android:id="@+id/ib_DietaryGoalPlus"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginBottom="2dp"
                    android:layout_marginRight="2dp"
                    android:background="@drawable/ic_list_add_green_normal"
                    android:visibility="gone" />
            </RelativeLayout>

            <!-- 주간 식이 그래프 0828 삭제

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="255dp"
                android:layout_marginLeft="2.5dp"
                android:layout_marginRight="2.5dp"
                android:layout_marginTop="3.5dp"
                android:background="@drawable/list_box_bg_green" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <RelativeLayout
                        android:id="@+id/ic_dietary_graph_title"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" >

                        <LinearLayout
                            android:layout_width="wrap_content"
