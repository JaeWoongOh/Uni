<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#DEDEDE" >

    <ScrollView
        android:id="@+id/scroll"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerInParent="true" >

        <LinearLayout
            android:id="@+id/ll_mainLinearLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#DEDEDE"
            android:orientation="vertical" >

            <!-- 날자 선택 영역 -->

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="32dp"
                android:background="@drawable/date_bar_bg_green" >

                <ImageButton
                    android:id="@+id/buttonBeforeDate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/date_bar_arrow_green_left_normal" />

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true" >

                    <TextView
                        android:id="@+id/textViewToday"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:text="@string/today"
                        android:textColor="#ffffff"
                        android:textSize="14.0dp" />

                    <TextView
                        android:id="@+id/textViewDate"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:textColor="#ffffff"
                        android:textSize="14.5dp" />
                </RelativeLayout>

                <ImageButton
                    android:id="@+id/buttonAfterDate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/date_bar_arrow_green_right_normal" />
            </RelativeLayout>

            <!-- 섭취 칼로리 영역 -->

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_162"
                android:background="@drawable/main_bg_green"
                android:orientation="horizontal" >

                <RelativeLayout
                    android:layout_width="158dp"
                    android:layout_height="132dp"
                    android:layout_marginTop="15dp" >

                    <TextView
                        android:id="@+id/tx_0_per"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="88dp"
                        android:text="100%"
                        android:textColor="#508026"
                        android:textSize="13dp" />
					<!--  -->
                    <TextView
                        android:id="@+id/tx_100_per"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="66dp"
                        android:text="100%"
                        android:textColor="#304c17"
                        android:textSize="13dp"
                        android:visibility="invisible" />

                    <!-- 그래프 영역 -->

                    <RelativeLayout
                        android:id="@+id/image_layout"
                        android:layout_width="123dp"
                        android:layout_height="144dp"
                        android:layout_below="@+id/tx_0_per"
                        android:layout_marginLeft="1dp"
                        android:layout_toRightOf="@+id/tx_100_per" >

                        <ImageView
                            android:id="@+id/dietary_main_thumnail"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:background="@drawable/img_top_graph1_green"
                            android:scaleType="fitXY" />

                        <LinearLayout
                            android:id="@+id/calorie_circle_graphview"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="1dp" android:orientation="horizontal">
                            </LinearLayout>
                    </RelativeLayout>

                    <LinearLayout
                        android:layout_width="158dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="50dp"
                        android:layout_marginTop="73dp"
                        android:gravity="center_horizontal"
                        android:orientation="horizontal" >

                            <TextView
                            android:id="@+id/cal_percents_textview"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:shadowColor="#B24787b2"
                            android:shadowDx="-1.5"
                            android:shadowDy="1.5"
                            android:shadowRadius="1.5"
                            android:text="0"
                            android:textColor="#ffffff"
                            android:textSize="23dp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="1dp"
                            android:layout_toRightOf="@id/cal_percents_textview"
                            android:shadowColor="#B24787b2"
                            android:shadowDx="-1.5"
                            android:shadowDy="1.5"
                            android:shadowRadius="1.5"
                            android:text="%"
                            android:textColor="#ffffff"
                            android:textSize="14dp" />
                    </LinearLayout>
                </RelativeLayout>
                <!-- 칼로리 수치 표시 영역 -->

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:layout_marginLeft="24dp"
                    android:layout_marginRight="27dp"
                    android:orientation="vertical" >

                    <LinearLayout
                        android:layout_width="152dp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:layout_marginTop="23dp"
                        android:gravity="right"
                        android:orientation="horizontal" >

                        <ImageView
                            android:id="@+id/iv_icon"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/ic_msg_btn_green_normal" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="6dp"
                            android:layout_marginTop="9dp"
                            android:text="@string/dietary_today_eat"
                            android:textColor="#304c17"
                            android:textSize="13dp"
                            android:textStyle="bold" />
                    </LinearLayout>

                    <TextView
                        android:id="@+id/textViewTodayTotalKcal"
                        android:layout_width="152dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="-15dp"
                        android:ellipsize="none"
                        android:gravity="right|top"
                        android:shadowColor="#25331A"
                        android:shadowDx="1"
                        android:shadowDy="2"
                        android:shadowRadius="4"
                        android:singleLine="true"
                        android:textColor="#ffffff"
                        android:textSize="50dp"
                        android:textStyle="bold" />

                    <ImageView
                        android:layout_width="152dp"
                        android:layout_height="0.50dp"
                        android:layout_marginTop="-5dp"
                        android:background="#33617f"
                        android:contentDescription="@string/blank" />

                    <LinearLayout
                        android:layout_width="152dp"
                        android:layout_height="wrap_content"
                        android:gravity="right"
                        android:orientation="horizontal" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/dietary_graph_title"
                            android:textColor="#304c17"
                            android:textSize="12.5dp" />

                        <TextView
                            android:id="@+id/textViewEncourageKcal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="6dp"
                            android:textColor="#40661f"
                            android:textSize="20dp" />

                        <TextView
                            android:id="@+id/textViewEncourage_Kcal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="3dp"
                            android:text="@string/kcal_1"
                            android:textColor="#304c17"
                            android:textSize="15dp" />
                    </LinearLayout>

                    <!-- 가이드에 따라 android:background="@drawable/img_main_rounded_blue" 삭제 -->
                </LinearLayout>
            </LinearLayout>

            <!-- 아침/점심/저녁/간식 -->

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="57dp"
                android:orientation="horizontal" >

                <!-- 아침 -->

                <LinearLayout
                    android:id="@+id/ll_cal_breakfast"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/sub_tab_normal"
                    android:gravity="center_horizontal"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/textViewMorning"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:textColor="#646464"
                        android:textSize="24dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="-3dp"
                        android:text="@string/breakfast"
                        android:textColor="#7d7d7d"
                        android:textSize="13dp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@drawable/sub_tab_normal" >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginBottom="10dp"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/dot_vertical_green" />
                </LinearLayout>

                <!-- 점심 -->

                <LinearLayout
                    android:id="@+id/ll_cal_lunch"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/sub_tab_normal"
                    android:gravity="center_horizontal"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/textViewAfternoon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:textColor="#646464"
                        android:textSize="24dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="-3dp"
                        android:text="@string/lunch"
                        android:textColor="#7d7d7d"
                        android:textSize="13dp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@drawable/sub_tab_normal" >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginBottom="10dp"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/dot_vertical_green" />
                </LinearLayout>

                <!-- 저녁 -->

                <LinearLayout
                    android:id="@+id/ll_cal_dinner"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/sub_tab_normal"
                    android:gravity="center_horizontal"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/textViewEvening"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:textColor="#646464"
                        android:textSize="24dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="-3dp"
                        android:text="@string/dinner"
                        android:textColor="#7d7d7d"
                        android:textSize="13dp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@drawable/sub_tab_normal" >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginBottom="10dp"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/dot_vertical_green" />
                </LinearLayout>

                <!-- 간식 -->

                <LinearLayout
                    android:id="@+id/ll_cal_snack"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/sub_tab_normal"
                    android:gravity="center_horizontal"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/textViewSnack"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:textColor="#646464"
                        android:textSize="24dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="-3dp"
                        android:text="@string/snack"
                        android:textColor="#7d7d7d"
                        android:textSize="13dp" />
                </LinearLayout>
            </LinearLayout>

            <!-- 피드백 메시지	08.28 삭제

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="47dp"
                android:background="@drawable/bg_notice_green"
                android:gravity="left" >

                <ImageView
                    android:id="@+id/imageViewIntakeFeedback"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="9dp"
                    android:src="@drawable/ic_notice_vegetable" />

                <TextView
                    android:id="@+id/textViewIntakeFeedback"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="6dp"
                    android:layout_marginRight="10dp"
